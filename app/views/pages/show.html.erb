<article>
  <header>
    <%= content_tag :h1, @page.title %>
    <%= content_tag :p do %>
      Authored by
      <% @page.authors.each do |author| %>
        <%= link_to author.name, author %>
      <% end %>
    <% end %>
  </header>
  <main>
    <%= content_tag :p, @page.text %>
  </main>
</article>
